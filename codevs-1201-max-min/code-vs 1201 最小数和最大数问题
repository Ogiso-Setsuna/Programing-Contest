/*
    code vs 1201 最小数和最大数问题
    http://codevs.cn/problem/1201/
*/

#include <iostream>
using namespace std;
int main()
{
    long a;
    int n;
    long max,min;
    cin>>n;
    cin>>a;
    max=min=a;
    for(int i=2;i<=n;i++)
    {
        cin>>a;
        if(a>max)
        max=a;
        if(a<min)
        min=a;
    }
    cout<<min<<" "<<max<<endl;
    return 0;
}
